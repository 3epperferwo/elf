{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "devtools": {
      "root": "packages/devtools",
      "sourceRoot": "packages/devtools/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/devtools",
            "main": "packages/devtools/src/index.ts",
            "tsConfig": "packages/devtools/tsconfig.lib.json",
            "assets": ["packages/devtools/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/devtools/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/devtools"],
          "options": {
            "jestConfig": "packages/devtools/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "entities": {
      "root": "packages/entities",
      "sourceRoot": "packages/entities/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/entities",
            "main": "packages/entities/src/index.ts",
            "tsConfig": "packages/entities/tsconfig.lib.json",
            "assets": ["packages/entities/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/entities/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/entities"],
          "options": {
            "jestConfig": "packages/entities/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "mocks": {
      "root": "packages/mocks",
      "sourceRoot": "packages/mocks/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/mocks/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/mocks"],
          "options": {
            "jestConfig": "packages/mocks/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ng": {
      "projectType": "application",
      "root": "apps/ng",
      "sourceRoot": "apps/ng/src",
      "prefix": "elf",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/ng",
            "index": "apps/ng/src/index.html",
            "main": "apps/ng/src/main.ts",
            "polyfills": "apps/ng/src/polyfills.ts",
            "tsConfig": "apps/ng/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/ng/src/favicon.ico", "apps/ng/src/assets"],
            "styles": [
              "apps/ng/src/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/ng/src/environments/environment.ts",
                  "with": "apps/ng/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ng:build:production"
            },
            "development": {
              "browserTarget": "ng:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ng/src/**/*.ts", "apps/ng/src/**/*.html"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ng"],
          "options": {
            "jestConfig": "apps/ng/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ng-e2e": {
      "root": "apps/ng-e2e",
      "sourceRoot": "apps/ng-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ng-e2e/cypress.json",
            "tsConfig": "apps/ng-e2e/tsconfig.e2e.json",
            "devServerTarget": "ng:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ng-e2e/**/*.{js,ts}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      }
    },
    "pagination": {
      "root": "packages/pagination",
      "sourceRoot": "packages/pagination/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/pagination",
            "main": "packages/pagination/src/index.ts",
            "tsConfig": "packages/pagination/tsconfig.lib.json",
            "assets": ["packages/pagination/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/pagination/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/pagination"],
          "options": {
            "jestConfig": "packages/pagination/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "persist-state": {
      "root": "packages/persist-state",
      "sourceRoot": "packages/persist-state/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/persist-state",
            "main": "packages/persist-state/src/index.ts",
            "tsConfig": "packages/persist-state/tsconfig.lib.json",
            "assets": ["packages/persist-state/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/persist-state/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/persist-state"],
          "options": {
            "jestConfig": "packages/persist-state/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "requests": {
      "root": "packages/requests",
      "sourceRoot": "packages/requests/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/requests",
            "main": "packages/requests/src/index.ts",
            "tsConfig": "packages/requests/tsconfig.lib.json",
            "assets": ["packages/requests/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/requests/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/requests"],
          "options": {
            "jestConfig": "packages/requests/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "state-history": {
      "root": "packages/state-history",
      "sourceRoot": "packages/state-history/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/state-history",
            "main": "packages/state-history/src/index.ts",
            "tsConfig": "packages/state-history/tsconfig.lib.json",
