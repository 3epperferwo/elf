    "husky": "6.0.0",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.2",
    "lint-staged": "11.1.2",
    "lodash": "4.17.21",
    "nx": "14.5.1",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
